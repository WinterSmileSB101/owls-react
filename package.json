{
    "name": "Generator",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development webpack-dev-server --config ./tools/scripts/build/webpack.development.ts --env dev --hot --open ",
        "build": "cross-env NODE_ENV=production webpack --config ./tools/scripts/build/webpack.production.ts --env prd",
        "test": "echo \"Error: no test specified\" && exit 1",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "lint": "npm run lint-eslint && npm run lint-stylelint",
        "lint:eslint": "eslint -c .eslintrc.js --exact .ts,.tsx,.js src",
        "lint:stylelint": "stylelint --config .stylelintrc.js static/styles/**/*.{less,css,scss}",
        "lint:commit": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "npm run lint:commit"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js}": [
            "npm run lint:eslint"
        ],
        "*.{css,less,scss}": [
            "npm run lint:stylelint"
        ],
        "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "ie >= 9",
        "not op_mini all"
    ],
